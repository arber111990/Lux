<% current_user.bookings.each do |booking| %>
<div class="card">
  <p><%= booking.item.title %> <%= booking.state %></p>

</div>
<% end %>



<% @user.items.each do |item| %>
  <%= item.title %>
  <% item.bookings.each do |booking| %>
    <%= booking.start_date %>
    <%= booking.end_date %>
    <% if @user.id == current_user.id %>
      <h1>buttons here</h1>
    <% end %>
  <% end %>
<% end %>
